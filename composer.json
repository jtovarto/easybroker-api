{
  "name": "easybroker/apiconnect",
  "description": "EasyBroker API connection package",
  "keywords": [
    "easeybroker"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.67",
    "guzzlehttp/guzzle": "^6.5|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^4.0|^5.0|^6.0"
  },
  "autoload": {
    "psr-4": {
      "EasyBroker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EasyBroker\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "authors": [
    {
      "name": "Johan Tovar",
      "email": "jtovarto@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
